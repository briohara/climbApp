":"../src/components/Login.js","original":{"line":70,"column":15}}],"sources":{"../src/components/Login.js":"import React, { useState, useEffect, useImperativeHandle } from \"react\";\r\nimport { Router, Link, navigate } from \"@reach/router\";\r\nimport jwt_Decode from \"jwt-decode\";\r\n\r\nimport { signIn } from \"../api/api\";\r\n\r\nconst Login = props => {\r\n  const [username, setUsername] = useState(\"\");\r\n  const [password, setPassword] = useState(\"\");\r\n\r\n  if (\r\n    jwt_Decode(localStorage.getItem(\"JWT\")).exp >\r\n    new Date().getTime() / 1000\r\n  ) {\r\n    console.log(\"Token is not expired.\");\r\n    navigate(\"userpage\");\r\n  }\r\n\r\n  function handleSubmit(e) {\r\n    e.preventDefault();\r\n\r\n    //try signing in\r\n    signIn({\r\n      username: e.target.username.value,\r\n      password: e.target.password.value\r\n    })\r\n      .then(res => {\r\n        localStorage.setItem(\"JWT\", res.token);\r\n\r\n        alert(\"Successful!!!\");\r\n        navigate(\"userpage\");\r\n      })\r\n      .catch(err => console.log(err));\r\n  }\r\n\r\n  return (\r\n    <div id=\"login\">\r\n      <form onSubmit={handleSubmit}>\r\n        <label htmlFor=\"username\">Username</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"username\"\r\n          placeholder=\"Username...\"\r\n          className=\"form-control\"\r\n          value={username}\r\n          onChange={e => setUsername(e.target.value)}\r\n          autoFocus\r\n          required\r\n        />\r\n        <label htmlFor=\"password\">Password</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"password\"\r\n          placeholder=\"Password...\"\r\n          className=\"form-control\"\r\n          value={password}\r\n          onChange={e => setPassword(e.target.value)}\r\n          required\r\n        />\r\n        <button type=\"submit\" className=\"btn btn-light\">\r\n          Submit\r\n        </button>\r\n      </form>\r\n\r\n      <Link to=\"/signup\">create an account.</Link>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n"},"lineCount":null}},"error":null,"hash":"455209c35e8538424ea8ddbfe6006832","cacheData":{"env":{}}}