ogin.js","original":{"line":64,"column":6}},{"generated":{"line":104,"column":26},"source":"../src/components/Login.js","original":{"line":36,"column":4}},{"generated":{"line":104,"column":27},"source":"../src/components/Login.js","original":{"line":35,"column":2}},{"generated":{"line":105,"column":0},"source":"../src/components/Login.js","original":{"line":67,"column":1}},{"generated":{"line":105,"column":1},"source":"../src/components/Login.js","original":{"line":8,"column":0}},{"name":"Login","generated":{"line":107,"column":15},"source":"../src/components/Login.js","original":{"line":69,"column":15}}],"sources":{"../src/components/Login.js":"import React, { useState, useEffect, useImperativeHandle } from \"react\";\r\nimport { Router, Link, navigate } from \"@reach/router\";\r\nimport { useCookies } from \"react-cookie\";\r\nimport jwt_Decode from \"jwt-decode\";\r\n\r\nimport { signIn } from \"../api/api\";\r\n\r\nconst Login = props => {\r\n  const [username, setUsername] = useState(\"\");\r\n  const [password, setPassword] = useState(\"\");\r\n  const [cookies, setCookie] = useCookies([\"jwt\"]);\r\n\r\n  if (jwt_Decode(cookies.JWT).exp > new Date().getTime() / 1000) {\r\n    console.log(\"Token is not expired.\");\r\n    navigate(\"userpage\");\r\n  }\r\n\r\n  function handleSubmit(e) {\r\n    e.preventDefault();\r\n\r\n    //try signing in\r\n    signIn({\r\n      username: e.target.username.value,\r\n      password: e.target.password.value\r\n    })\r\n      .then(res => {\r\n        setCookie(\"JWT\", res.token, { path: \"/\" });\r\n\r\n        alert(\"Successful!!!\");\r\n        navigate(\"userpage\");\r\n      })\r\n      .catch(err => console.log(err));\r\n  }\r\n\r\n  return (\r\n    <div id=\"login\">\r\n      <form onSubmit={handleSubmit}>\r\n        <label htmlFor=\"username\">Username</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"username\"\r\n          placeholder=\"Username...\"\r\n          className=\"form-control\"\r\n          value={username}\r\n          onChange={e => setUsername(e.target.value)}\r\n          autoFocus\r\n          required\r\n        />\r\n        <label htmlFor=\"password\">Password</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"password\"\r\n          placeholder=\"Password...\"\r\n          className=\"form-control\"\r\n          value={password}\r\n          onChange={e => setPassword(e.target.value)}\r\n          required\r\n        />\r\n        <button type=\"submit\" className=\"btn btn-light\">\r\n          Submit\r\n        </button>\r\n      </form>\r\n\r\n      <Link to=\"/signup\">create an account.</Link>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n"},"lineCount":null}},"error":null,"hash":"6f081686b1419d7b608e3db879c00c2c","cacheData":{"env":{}}}